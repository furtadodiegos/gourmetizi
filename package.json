{
  "name": "gourmetizi",
  "version": "1.0.4-4",
  "scripts": {
    "prebuild": "expo prebuild --clean -p all",
    "start": "expo start --dev-client",
    "server": "tsx watch ./server",
    "postinstall": "yarn generate:gql:types",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint ./src --ext .ts,.tsx --fix",
    "prettify": "prettier --write ./src",
    "test": "jest",
    "generate:gql:schema": "yarn graphql-codegen --config ./codegen.schema.yml -r dotenv/config",
    "generate:gql:types": "yarn graphql-codegen --config ./codegen.types.yml && yarn lint"
  },
  "dependencies": {
    "@apollo/client": "^3.7.4",
    "@expo/config-plugins": "^5.0.4",
