{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "channel": "production",
      "ios": { "resourceClass": "m1-medium" },
      "env": { "APP_ENV": "production" },
      "cache": { "key": "invalidate1" }
    },
    "development": {
      "extends": "production",
      "developmentClient": true,
      "distribution": "internal",
      "env": { "APP_ENV": "development" }
    },
    "staging": {
      "extends": "production",
      "channel": "staging",
      "env": { "APP_ENV": "staging" }
    }
  },
  "submit": {
    "production": {},
    "staging": {
      "android": {
        "serviceAccountKeyPath": "./android/google-services.json",
        "track": "internal",
        "applicationId": "com.dfti.gourmetizi",
        "releaseStatus": "completed"
      },
      "ios": {
        "appleId": "dfti.apps@gmail.com",
        "appleTeamId": "9K5YUJ96KS",
        "bundleIdentifier": "com.dfti.gourmetizi",
        "ascAppId": "1665180188"
      }
    }
  }
}
